// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Repository {
  id           String       @id @default(uuid())
  remote       String
  private      Boolean      @default(false)
  valid        Boolean?
  last_pull    DateTime?
  ref_bytes    Bytes?
}